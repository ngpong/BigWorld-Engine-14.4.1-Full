<root>

	<NestedArray> FIXED_DICT
		<Properties>
			<inner>
				<Type> ARRAY <of> INT32 </of> </Type>
			</inner>
		</Properties>
		<AllowNone> True </AllowNone>
	</NestedArray>

	<ENUM>          INT8        </ENUM>

	<FRIENDIDX>			UINT8		</FRIENDIDX>

	<BOOL>				UINT8
		<!-- BOOL uses the ENUM widget to show True for 1 and False for 0
		-->
		<Widget>	ENUM
			<enum>
				<value>	0	</value>
				<display>	False	</display>
			</enum>
			<enum>
				<value>	1	</value>
				<display>	True	</display>
			</enum>
		</Widget>
	</BOOL>

	<TEST_FIXED_DICT> FIXED_DICT
      <implementedBy> TestFixedDict.instance </implementedBy>
      <Properties>
          <a>
              <Type> INT32   </Type>
          </a>
          <b>
              <Type> STRING  </Type>
          </b>
          <c>
              <Type> ARRAY<of>UINT8</of> </Type>
          </c>
      </Properties>
      <Default>
          <a>    15 </a>
          <b>    TestType </b>
          <c>
              <item> 5. </item>
              <item> 12. </item>
              <item> 13. </item>
          </c>
      </Default>
    </TEST_FIXED_DICT>

	<TEST_USER_TYPE> USER_TYPE
		<implementedBy> TestUserType.instance </implementedBy>
    </TEST_USER_TYPE>
</root>
